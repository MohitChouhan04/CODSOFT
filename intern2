
#include<iostream>
using namespace std;
int main (){
	int a,b;
	char oper;
	cout<<"Enter any operator (+,-,*,/)=  ";
	cin>>oper;
	cout<<"Enter two number";
	cin>>a>>b;
	
	switch (oper){
		case '+':
		cout<<a+b;
		break;
	    case '-':
	    cout<<a-b;
	    break;
	    case '*':
	    cout<<a*b;
	    break;
	    case '/':
	    cout<<a/b;
	    break;
	    default:
	    	cout<<"opertor does not exist";
}
}
